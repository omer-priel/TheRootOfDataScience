{
    "domain": "www.yelp.com",
    "url": "https://www.yelp.com",
    "robots.txt": "https://www.yelp.com/robots.txt",
    "api": "https://www.yelp.com/developers/documentation/v3/business_search",
    "api_in_github": "https://github.com/Yelp/yelp-python",


    "All Locations": "https://www.yelp.com/locations",
    "Business Search": "https://www.yelp.com/search?find_loc={location}&start={start}",
    "Business Search Category": "https://www.yelp.com/search?find_desc={category}find_loc={location}&&start={start}",
    "Business Format": "https://www.yelp.com/biz/{sub-name}",


    "examples": [
        "https://www.yelp.com/biz/lily-san-francisco?osq=Restaurants",
        "https://www.yelp.com/biz/dumpling-park-san-francisco-7",
        "https://www.yelp.com/biz/merchant-roots-san-francisco?osq=Restaurants"
    ]
}